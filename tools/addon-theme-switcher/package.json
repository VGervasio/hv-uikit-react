{
  "name": "@hitachivantara/addon-theme-switcher",
  "version": "0.0.0",
  "private": true,
  "description": "Storybook addon Theme Switcher",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "license": "Apache-2.0",
  "keywords": [
    "hitachi-vantara",
    "ui-kit",
    "react",
    "storybook",
    "theme-switcher"
  ],
  "author": {
    "name": "Hitachi Vantara UI Kit Team"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc-watch --onSuccess 'pnpm build'",
    "build": "npm-run-all clean build:*",
    "build:esm": "tsc --module es2015 --outDir dist/esm && tsc-alias --dir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs && tsc-alias --dir dist/cjs",
    "clean": "npx rimraf ./dist"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@storybook/addons": "^6.4.20",
    "@storybook/api": "^6.4.20",
    "@storybook/components": "^6.4.20",
    "@storybook/core-events": "^6.4.20",
    "@storybook/theming": "^6.4.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsc-alias": "^1.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@hitachivantara/uikit-core": "workspace:*"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "npm-run-all": "^4.1.5",
    "tsc-alias": "^1.6.5",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  }
}
