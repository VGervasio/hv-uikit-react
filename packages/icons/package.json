{
  "name": "@hitachivantara/uikit-react-icons",
  "version": "5.0.0",
  "private": false,
  "description": "A collection of icons for use with Hitachi Vantara's React Components.",
  "homepage": "https://github.com/lumada-design/hv-uikit-react",
  "license": "Apache-2.0",
  "keywords": [
    "hitachi-vantara",
    "design-system",
    "ui-kit",
    "react",
    "typescript",
    "wai-aria"
  ],
  "author": {
    "name": "Hitachi Vantara UI Kit Team"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumada-design/hv-uikit-react.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx npm-run-all --npm-path pnpm --serial clean svg-optimize build:*",
    "build:convert": "node ./src/svgToReact.js dir --input assets/ --output bin --force",
    "build:copy": "npx cpy-cli \"src/IconBase*\" \"bin\"",
    "build:vite": "tsc && vite build",
    "svg-optimize": "npx svgo --config=./svgo.config.js -r -f assets assets",
    "clean": "npx rimraf dist bin"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@hitachivantara/uikit-styles": "workspace:*",
    "chokidar-cli": "^3.0.0",
    "recursive-readdir": "^2.2.3",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.0",
    "vite-tsconfig-paths": "^3.5.2",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "htmltojsx": "^0.3.0",
    "jsdom-no-contextify": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "svgo": "^3.0.0",
    "typescript": "^4.8.4"
  }
}
